        style(type='text/css').
            body {
                background: #F0F4F5;
                padding: 5px;
                color: #09425A;
                font-family: "Lucida Sans Unicode, Lucida Grande, sans-serif"	
            }

            #nurseLink, #patientLink{
                text-align: center;
                color: #09425A;
                background-color: #AABEC6;
                border-radius: 15px;
                font-size-adjust: 0.58;
            }

            #doctorNoteArea, .card {
                border-radius: 15px;
            }

            .card{
                background: #009ABB;
                color: #f5f6fa;
                margin-bottom: 15px;
            }

            .navbar {
                text-align: center;
                list-style-type: none;
            }

            #search-button{
                border-left: 1em !important;
            }

            #search-bar{
                width: 50%;
            }

            .fa-plus{
                font-size: 3em;
                color: #8C1515;
                margin-right: 15px;
            }

            #patientsIcon, #nursesIcon{
                font-size: 1.4em;
            }

            #sidebarTitle{
                margin-left: 15px;
            }

            #patientsList, #nursesList {
                border: none !important;
                padding: 0 !important;
                margin: 5px;
            }

            #nursesIcon{
                padding-top: 20px;
            }

            .list-group-item, #patient {
                background: #f5f6fa;
                border: none !important;
                padding: 0 !important;
                margin: 3px;
            }

            #loggedin {
                margin-right: 5px;
            }

            h4{
                display: inline-block;
            }

            .sticky-top, #main {
                margin-top: 20px;
            }

            .td, .th {
                border-radius:15px !important;
            }

            #table-head{
                font-size-adjust: 1;
                text-decoration: underline;
            }

            #DIV_1 {
                background-position: 50% 100%;
                bottom: 0px;
                color: rgb(112, 111, 111);
                height: 192px;
                left: 0px;
                position: relative;
                right: 0px;
                text-decoration: none solid rgb(112, 111, 111);
                top: 0px;
                width: 1056px;
                column-rule-color: rgb(112, 111, 111);
                perspective-origin: 528px 96px;
                transform-origin: 528px 96px;
                caret-color: rgb(112, 111, 111);
                background: rgba(0, 0, 0, 0) url("https://www.floridahospital.com/sites/all/themes/custom/fh_system/images/bg-slogan.jpg") no-repeat scroll 50% 100% / auto padding-box border-box;
                border: 0px none rgb(112, 111, 111);
                font: normal normal 400 normal 16px / 24px Helmet, Freesans, sans-serif;
                outline: rgb(112, 111, 111) none 0px;
            }/*#DIV_1*/

            #DIV_2 {
                bottom: 0px;
                color: rgb(112, 111, 111);
                height: 151px;
                left: 0px;
                position: relative;
                right: 0px;
                text-decoration: none solid rgb(112, 111, 111);
                top: 0px;
                width: 1056px;
                column-rule-color: rgb(112, 111, 111);
                perspective-origin: 528px 75.5px;
                transform-origin: 528px 75.5px;
                caret-color: rgb(112, 111, 111);
                border: 0px none rgb(112, 111, 111);
                font: normal normal 400 normal 16px / 24px Helmet, Freesans, sans-serif;
                outline: rgb(112, 111, 111) none 0px;
            }/*#DIV_2*/

            #DIV_3, #DIV_4 {
                color: rgb(112, 111, 111);
                height: 151px;
                text-decoration: none solid rgb(112, 111, 111);
                width: 1056px;
                column-rule-color: rgb(112, 111, 111);
                perspective-origin: 528px 75.5px;
                transform-origin: 528px 75.5px;
                caret-color: rgb(112, 111, 111);
                border: 0px none rgb(112, 111, 111);
                font: normal normal 400 normal 16px / 24px Helmet, Freesans, sans-serif;
                outline: rgb(112, 111, 111) none 0px;
            }/*#DIV_3, #DIV_4*/

            #DIV_3:after, #DIV_4:after {
                clear: both;
                color: rgb(112, 111, 111);
                content: '""';
                display: table;
                height: 0px;
                text-decoration: none solid rgb(112, 111, 111);
                visibility: hidden;
                width: 0px;
                column-rule-color: rgb(112, 111, 111);
                perspective-origin: 0px 0px;
                transform-origin: 0px 0px;
                caret-color: rgb(112, 111, 111);
                border: 0px none rgb(112, 111, 111);
                font: normal normal 400 normal 16px / 24px Helmet, Freesans, sans-serif;
                outline: rgb(112, 111, 111) none 0px;
            }/*#DIV_3:after, #DIV_4:after*/

            #DIV_3:before, #DIV_4:before {
                color: rgb(112, 111, 111);
                display: table;
                height: 0px;
                text-decoration: none solid rgb(112, 111, 111);
                width: 0px;
                column-rule-color: rgb(112, 111, 111);
                perspective-origin: 0px 0px;
                transform-origin: 0px 0px;
                caret-color: rgb(112, 111, 111);
                border: 0px none rgb(112, 111, 111);
                font: normal normal 400 normal 16px / 24px Helmet, Freesans, sans-serif;
                outline: rgb(112, 111, 111) none 0px;
            }/*#DIV_3:before, #DIV_4:before*/

            #H6_5 {
