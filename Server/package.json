    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "jshint": "^2.9.5",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "pug-lint": "^2.5.0",
    "supertest": "^3.0.0"
