{
  "name": "HospitalHorror",
  "main": "server.js",
  "scripts": {
    "start": "set NODE_ENV=development&&node ./server.js",
    "test": "npm run lint-test && npm run mocha-test",
    "create_test_db": "NODE_ENV=test node ./database/resync.js && NODE_ENV=test node ./database/loadTestData.js",
    "mocha-test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --recursive --require babel-core/register --exit",
    "lint-test": "jshint server.js app/ config/ database/ test/ && pug-lint views/",
    "resync": "node ./database/resync.js",
    "load_testdata": "node ./database/loadTestData.js"
  },
  "dependencies": {
    "async": "^2.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "dottie": "^2.0.0",
    "express": "^4.16.2",
    "express-formidable": "^1.0.0",
    "express-session": "^1.15.6",
    "graceful-fs": "^4.1.11",
    "inflection": "^1.12.0",
    "js-sha3": "^0.6.1",
    "lodash": "^4.17.5",
    "method-override": "^2.3.10",
    "moment": "^2.22.1",
    "mysql2": "^1.5.2",
    "npm": "^6.0.0",
    "octicons": "^7.2.0",
    "pug": "^2.0.3",
    "sequelize": "^4.37.5",
    "session-file-store": "^1.2.0",
    "stylelint-config-standard": "^17.0.0",
    "wkx": "^0.4.4",
    "xmlhttprequest": "^1.8.0"
